{
	"extends": ["config:base"],
	"automerge": true,
	"semanticCommits": true,
	"labels": ["dependencies"],
	"packageRules": [
			{
			"matchPackagePatterns": [
			"*"
			],
			"matchUpdateTypes": [
			"minor",
			"patch"
			],
			"groupName": "all non-major dependencies",
			"groupSlug": "all-minor-patch"
    	}
  ]
}
